        "/api/getAllContactInfo": {
            "get": {
                "tags": [
                    "Contact Infomation"
                ],
                "summary": "getAllContactInfo",
                "description": "get all contact information",
                "operationId": "getAllContactInfo",
                "requestBody": {},
                "responses": {
                    "200": {
                        "description": "response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "array"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/createContactInfo": {
            "post": {
                "tags": [
                    "Contact Infomation"
                ],
                "summary": "Create Contact Info",
                "description": "Create Contact Info",
                "operationId": "createContactInfo",
                "requestBody": {
                    "description": "create information",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "gender",
                                    "email",
                                    "phone_number"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "wan ahamd"
                                    },
                                    "gender": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "Lelaki / Perempuan"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "user1@mail.com"
                                    },
                                    "phone_number": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "+601928884992"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Wrong credentials response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sorry, wrong email address Please try again"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
